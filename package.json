{
  "name": "community-modules-viewer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/generate-version.js && node scripts/fetch-builtin-modules.js && node scripts/generate-changelog.js && node scripts/populate-by-module.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:e2e:prod",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:all": "npm run test:unit && npm run test:e2e:prod",
    "test:e2e": "playwright test",
    "test:e2e:prod": "./scripts/test-e2e.sh",
    "test:e2e:dev": "playwright test --config=playwright.config.js",
    "fetch:modules": "node scripts/fetch-builtin-modules.js",
    "generate:changelog": "node scripts/generate-changelog.js && node scripts/populate-by-module.js",
    "docker:up": "docker compose up",
    "docker:build": "docker compose build",
    "docker:down": "docker compose down",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build",
    "docker:prod": "docker compose -f docker-compose.prod.yml up",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gettakaro/community-modules-viewer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/gettakaro/community-modules-viewer/issues"
  },
  "homepage": "https://github.com/gettakaro/community-modules-viewer#readme",
  "dependencies": {
    "@playwright/test": "^1.53.2",
    "@takaro/apiclient": "^0.4.0",
    "axios": "^1.12.2",
    "next": "^15.3.4",
    "prismjs": "^1.29.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.7",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "daisyui": "^4.12.10",
    "eslint-config-next": "^15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
